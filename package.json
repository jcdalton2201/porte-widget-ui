{
  "name": "porte-widget-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bs": "browser-sync start -c ./bs-config.cjs",
    "build": "rollup -c",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "cc": "node scripts/createComponent.js",
    "lint": "eslint src --ignore-pattern *.stories.* --fix",
    "postcc": "npm run lint",
    "prebuild": "npm-run-all update-buildfile lint",
    "pretest": "npm run build",
    "prework": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jasmine spec/**/*.js",
    "watchFiles": "rollup -c",
    "start": "npm-run-all -p bs watch",
    "predeploy": "npm run build",
    "deploy": "cpx \"./dist/*.js\" ../ncaa-pool-app/public",
    "update-buildfile": "node scripts/buildfile.js",
    "watch": "watch \"npm run watchFiles\" src/"
  },
  "author": "Jason Dalton",
  "license": "MIT",
  "dependencies": {
    "@populus/squid": "^0.0.35",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.5",
    "chalk": "^4.1.2",
    "cpx": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^12.0.2",
    "lit": "^2.0.0",
    "marked": "^3.0.4",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "node-sass": "^6.0.1",
    "node-vault": "^0.9.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "proxy-middleware": "^0.15.0",
    "rollup": "^2.57.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-postcss-lit": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.1",
    "watch": "^1.0.2"
  }
